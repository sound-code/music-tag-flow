{
  "dataSources": {
    "primary": {
      "enabled": true,
      "type": "json",
      "config": {
        "tracksFile": "./data/tracks.json"
      }
    },
    "backup": {
      "enabled": false,
      "type": "rest_api",
      "config": {
        "baseUrl": "https://api.example.com/music",
        "apiKey": "",
        "endpoints": {
          "tracks": "/tracks",
          "artists": "/artists",
          "albums": "/albums"
        }
      }
    }
  },
  "options": {
    "cacheEnabled": true,
    "cacheTimeout": 300000,
    "retryAttempts": 3,
    "fallbackEnabled": true
  }
} 